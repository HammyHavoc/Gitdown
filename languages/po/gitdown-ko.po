msgstr "WordPress에 있습니다"

msgid "Not on wordpress"
msgstr "WordPress가 아닙니다"

msgid "Update"
msgstr "업데이트"

msgid "Delete"
msgstr "삭제"

msgid "Open in new Tab"
msgstr "새 탭에서 열립니다"

